{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>renovatebot/.github"],
  "regexManagers": [
      {
      "fileMatch": ["bins-versions"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>[a-z-]+?)(?: (?:packageName|lookupName)=(?<packageName>.+?))?(?: versioning=(?<versioning>[a-z-]+?))?\\s(?<depName>[a-z-]+?) (?<currentValue>.+?)\\s"
      ]
    }
   ]
}
